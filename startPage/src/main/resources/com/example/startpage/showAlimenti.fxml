<AnchorPane maxHeight="720.0" maxWidth="1080.0" onMouseMoved="#mostraData" prefHeight="720.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.startpage.Controller">
   <children>
      <ImageView fitHeight="720.0" fitWidth="1080.0" pickOnBounds="true">
         <image>
            <Image url="@DarkTheme.png" />
         </image>
      </ImageView>
      <Button fx:id="indietro2" layoutX="21.0" layoutY="656.0" mnemonicParsing="false" onAction="#back2" prefHeight="32.0" prefWidth="72.0" style="-fx-background-color: azure;" text="Indietro" />
      <Button fx:id="newAlimento" layoutX="110.0" layoutY="656.0" mnemonicParsing="false" onAction="#addAlimento" prefHeight="32.0" prefWidth="141.0" style="-fx-background-color: azure;" text="Aggiungi un alimento" />
      <ScrollPane fx:id="scrollAlim" fitToHeight="true" hbarPolicy="NEVER" hmax="0.0" layoutX="140.0" layoutY="55.0" maxHeight="500.0" maxWidth="800.0" prefHeight="500.0" prefWidth="800.0" vbarPolicy="ALWAYS" vmax="5.0" vvalue="1.0">
        <content>
          <AnchorPane fx:id="ancorpane" maxHeight="500.0" maxWidth="800.0" minHeight="0.0" minWidth="0.0" prefHeight="500.0" prefWidth="800.0">
               <children>
                  <Text fx:id="textAlim" layoutX="34.0" layoutY="32.0" onMouseMoved="#mostraData" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="737.0">
